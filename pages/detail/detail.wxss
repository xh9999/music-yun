page {
  height: 100%;
}
.container {
  display: flex;
  flex-direction: column;
  height: 100%;
}
.container .titleImg {
  width: 100%;
  height: 380rpx;
}
.container .titleImg .img {
  width: 100%;
  height: 100%;
}
.container .scroll {
  flex: 1;
  overflow: hidden;
  background-color: #000;
}
.container .scroll .contents .van-cell {
  background-color: #313131;
  color: #fff;
  height: 150rpx;
}
.container .scroll .contents .van-cell .van-cell_title {
  background-color: #222;
  padding: 8rpx;
}
.container .scroll .contents .cell {
  background-color: #000;
  display: flex;
}
.container .scroll .contents .cell .icon {
  height: 100rpx;
  width: 100rpx;
  background-color: #000;
  margin-right: 40rpx;
}
.container .scroll .contents .cell .icon image {
  width: 100%;
  height: 100%;
}
.container .scroll .contents .cell .content {
  width: 545rpx;
  position: relative;
}
.container .scroll .contents .cell .content view {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.container .scroll .contents .cell .content .one {
  width: 80%;
}
