page {
  height: 100%;
  background-color: #000;
}
.self .title {
  position: relative;
  width: 100%;
  height: 400rpx;
  background-color: #202020;
}
.self .title .icon {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 300rpx;
  height: 168rpx;
}
.self .title .icon .img {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
}
.self .title .icon .img .login {
  position: relative;
  width: 100%;
  height: 100%;
}
.self .title .icon .img image {
  width: 100%;
  height: 100%;
}
.self .title .icon .cot {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  width: 300rpx;
  text-align: center;
  color: #fff;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.self .van-cell {
  color: #fff;
  background-color: #000;
  height: 120rpx;
  line-height: 120rpx;
  padding-top: 0;
  padding-bottom: 0;
}
.self .van-cell__right-icon-wrap {
  margin-top: 39rpx;
}
.self .cell {
  background-color: #000;
  display: flex;
}
.self .cell .icon {
  height: 100rpx;
  width: 100rpx;
}
.self .cell .icon image {
  width: 100%;
  height: 100%;
}
.self .cell .content {
  width: 545rpx;
  position: relative;
}
.self .cell .content view {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.self .cell .content .one {
  width: 80%;
  height: 50%;
  line-height: 51rpx;
  margin-left: 40rpx;
}
@font-face {
  font-family: 'iconfont';
  /* Project id 2907777 */
  src: url('//at.alicdn.com/t/font_2907777_bjs2ns47sy.woff2?t=1636157461096') format('woff2'), url('//at.alicdn.com/t/font_2907777_bjs2ns47sy.woff?t=1636157461096') format('woff'), url('//at.alicdn.com/t/font_2907777_bjs2ns47sy.ttf?t=1636157461096') format('truetype');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-woxihuande_on:before {
  content: "\e670";
}
.icon-denglu:before {
  position: absolute;
  content: "\e67c";
  color: aliceblue;
  top: 60%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 87rpx;
}
